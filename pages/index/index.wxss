.tabbar {
  position: fixed;
  left: 0;
  bottom: 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 750rpx;
  height: 120rpx;
  background-color: white;
}
.tabbar .tabbar-item {
  width: 20%;
}
.tabbar .tabbar-item .container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.tabbar .tabbar-item .container.big-icon .tabbar-pic {
  transform: scale(1.5) translateY(-25%);
  border: 10rpx solid white;
  border-radius: 50%;
  box-shadow: 0 -1px 0 #dddddd80;
}
.tabbar .tabbar-item .container.big-icon .tabbar-name {
  transform: translateY(-25%);
}
.tabbar .tabbar-item .container .tabbar-pic {
  width: 56rpx;
  height: 56rpx;
}
.tabbar .tabbar-item .container .tabbar-name {
  color: #666;
  font-size: 20rpx;
}
.tabbar .tabbar-item.active .tabbar-name {
  color: #07c160;
}
.page-header-wrap {
  background-color: white;
}
.page-header {
  position: fixed;
  left: 0;
  top: 0;
  width: 750rpx;
  z-index: 9999;
}
.page-header .status-bar {
  background-color: white;
}
.page-header .nav {
  position: relative;
  display: flex;
  padding: 0 20rpx;
  align-items: center;
  background-color: var(--title-bg-color);
}
.page-header .message-wrap {
  position: relative;
  width: 60rpx;
  height: 60rpx;
}
.page-header .message-wrap .icon {
  position: absolute;
  padding: 0 8rpx;
  top: 0;
  right: 0;
  transform: translate(25%, -25%);
  color: white;
  font-size: 24rpx;
  background-color: red;
  border-radius: 50%;
}
.page-header .message {
  width: 60rpx;
  height: 60rpx;
}
.page-header .title {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: white;
}
.banner-img {
  width: 100%;
  height: 100%;
}
.fun-list {
  display: flex;
  padding: 20rpx 0;
  background-color: #001938;
  color: white;
}
.fun-item {
  width: 20%;
  text-align: center;
}
.fun-pic {
  width: 100rpx;
  height: 100rpx;
}
.fun-two {
  margin-top: 20rpx;
  display: grid;
  grid-template-columns: repeat(4, 25%);
  background-color: white;
}
.fun-two-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 187.5rpx;
  height: 187.5rpx;
  box-sizing: border-box;
  text-align: center;
  border-right: 1px solid #ccc;
  border-top: 1px solid #ccc;
}
.fun-two-item:nth-child(4n) {
  border-right: unset;
}
.fun-two-item:nth-child(-n+4) {
  border-top: unset;
}
.fun-two-pic {
  display: block;
  margin: 0 auto;
  width: 120rpx;
  height: 120rpx;
}
.popup-box {
  width: 700rpx;
  background-color: white;
  border-radius: 10rpx;
  overflow: hidden;
}
.popup-box .title {
  padding: 20rpx 0;
  text-align: center;
  color: #07c160;
  font-size: 36rpx;
  font-weight: 400;
  background-color: var(--title-bg-color);
}
.popup-box .func-list3 {
  display: flex;
  flex-wrap: wrap;
  background-color: white;
  padding: 20rpx 0;
}
.popup-box .func-list3 .popup-fun-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 170rpx;
  height: 170rpx;
}
page {
  background-color: #f2f2f2;
}
